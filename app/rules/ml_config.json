{
  "enabled": true,
  "use_mock": false,
  "zero_shot": {
    "model": "joeddav/xlm-roberta-large-xnli",
    "device": "auto",
    "batch_size": 8,
    "gate_min": 0.3,
    "gate_soft": 0.2,
    "langs": [
      "fr",
      "en",
      "multi"
    ]
  },
  "ner": {
    "device": "auto",
    "batch_size": 16,
    "registry": {
      "fr": "CATIE-AQ/NERmembert-large-3entities",
      "en": "dslim/bert-base-NER",
      "multi": "Davlan/xlm-roberta-base-ner-hrl"
    }
  },
  "enable_multilingual_fallback": true,
  "enable_auto_download": true,
  "hf_cache_dir": ".hf_cache",
  "max_total_model_ram_mb": 1800,
  "eviction_policy": "lru",
  "lite_switch": {
    "enabled": false,
    "zero_shot_model": "MoritzLaurer/mDeBERTa-v3-base-mnli-xnli"
  },
  "labels": {
    "experiences": [
