    "force_cpu": true,
    "local_only": false,
    "cache_dir": ".hf_cache",
    "batch_overrides": {
      "cpu": {
        "zero_shot": 4,
        "ner": 8
      },
      "cuda": {
        "zero_shot": 8,
        "ner": 16
      }
    }
  }
