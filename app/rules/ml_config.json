      "école"
    ],
    "skills": [
      "compétences techniques",
      "skills",
      "tech stack",
      "hard skills"
    ],
    "soft_skills": [
      "qualités",
      "soft skills",
      "comportementales"
    ],
    "languages": [
      "langues",
      "languages"
    ],
    "projects": [
      "projets",
      "projects",
      "réalisations",
      "portfolio"
    ],
    "certifications": [
      "certifications",
      "certification",
      "certif"
    ],
    "publications": [
      "publications",
      "articles",
      "papers"
    ],
    "volunteering": [
      "bénévolat",
      "volontariat",
      "associatif",
      "ngo"
    ],
    "interests": [
      "centres d'intérêt",
